<p id="notice"><%= notice %></p>
<h1>Listing All Plans</h1>
 

<%= link_to 'New Plan', new_plan_path %>
<br>
<table>
  <tr>
    <th>Name</th>
    <th>Fee</th>
    <th colspan="2"></th>
  </tr>
 
  <% @plans.each do |plan| %>
    <tr>
      <td><%= plan.name %></td>
      <td><%= plan.fee %></td>
      <td><%= link_to 'Show', plan_path(plan) %></td>
      <td><%= link_to 'Edit', edit_plan_path(plan) %></td>
    </tr>
  <% end %>
</table>
<br>

<li><%= link_to 'Back', admins_path %></li>

